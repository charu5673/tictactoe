<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center_horizontal"
    android:background="@drawable/picsart_23_07_15_00_01_50_736"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:weightSum="500">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="338"
            android:orientation="vertical"
            android:weightSum="338">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="111"
                android:orientation="vertical">

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="182"
                android:orientation="horizontal"
                android:weightSum="200">

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="38"
                    android:orientation="vertical">

                </LinearLayout>

                <TableLayout
                    android:id="@+id/tableLayout"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="124"
                    android:background="@drawable/gridb"
                    android:weightSum="3">

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="top"
                        android:weightSum="3">

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/lt"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="lt"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

                            <ImageView
                                android:id="@+id/lti"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/cti"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/ct"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="ct"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/rti"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/rt"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="rt"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="bottom|center_vertical"
                        android:weightSum="3">

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/lm"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="lm"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

                            <ImageView
                                android:id="@+id/lmi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/cmi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/cm"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:text="cm"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/rmi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/rm"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="rm"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>
                    </TableRow>

                    <TableRow
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        android:layout_weight="1"
                        android:gravity="bottom"
                        android:weightSum="3">

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/ldi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/ld"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="ld"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/cdi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/cd"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="cd"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>

                        <FrameLayout
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="1">

                            <ImageView
                                android:id="@+id/rdi"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="10dp" />

                            <com.google.android.material.button.MaterialButton
                                android:id="@+id/rd"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:hapticFeedbackEnabled="true"
                                android:text="rd"
                                android:textColor="#00FFFFFF"
                                app:backgroundTint="#00FFFFFF"
                                tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck,SpeakableTextPresentCheck" />
                        </FrameLayout>
                    </TableRow>

                </TableLayout>

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="38"
                    android:orientation="vertical"></LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="45"
                android:orientation="vertical"></LinearLayout>
        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="162">

            <FrameLayout
                android:layout_width="285dp"
                android:layout_height="136dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent">

                <ImageView
                    android:id="@+id/imageView14"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:src="@drawable/pl2" />

                <TextView
                    android:id="@+id/sc2"
                    android:layout_width="258dp"
                    android:layout_height="103dp"
                    android:layout_gravity="bottom|right"
                    android:textColor="#FFFFFF"
                    android:textSize="50sp" />
            </FrameLayout>

            <FrameLayout
                android:id="@+id/frameLayout"
                android:layout_width="254dp"
                android:layout_height="108dp"
                android:weightSum="100"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <ImageView
                    android:id="@+id/imageView13"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom|top"
                    android:src="@drawable/pl1" />

                <TextView
                    android:id="@+id/sc1"
                    android:layout_width="87dp"
                    android:layout_height="87dp"
                    android:layout_gravity="bottom|right"
                    android:textColor="#FFFFFF"
                    android:textSize="48sp" />
            </FrameLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>


    </LinearLayout>

</LinearLayout>
